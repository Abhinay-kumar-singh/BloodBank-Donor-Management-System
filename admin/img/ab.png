    <script type="text/javascript">
        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires="+d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";domain=.alphacoders.com;path=/;secure";
        }

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        $(document).ready(function() {
            var cookie_status = getCookie("cookieconsent_status");
            if(cookie_status == "" || cookie_status == "deny") {
                $('#cookie_consent').css('display', 'flex');
            }

            $('#change_consent').on('click', function () {
                $('#cookie_consent').css('display', 'flex');
            });

            $('#cookie_consent .btn-allow').on('click', function () {
                setCookie('cookieconsent_status', 'allow', 365);
                $('#cookie_consent').css('display', 'none');
            });

            $('#cookie_consent .btn-deny').on('click', function () {
                setCookie('cookieconsent_status', 'deny', 365);
                window.location.href = "https://alphacoders.com/site/no-consent";
            });

            if($("#sorting_options").length) {
                $("#sorting_options").on('shown.bs.collapse', function() {
                    fixVideoAd('shown', $('#sorting_options').outerHeight());
                }).on('hide.bs.collapse', function() {
                    fixVideoAd('hidden', $('#sorting_options').outerHeight());
                });
            }

            //Show/hide resolution info for the avatar
            $(".avatar-thumb").on("mouseenter", function() {
                $(".boxcaption", this).stop().animate({top: "165px" },{queue:false,duration:160});
            }).on("mouseleave", function() {
                $(".boxcaption", this).stop().animate({top: "200px" },{queue:false,duration:160});
            });

            $('body').on('click', '.close-notice', function() {
                $(this).parent().fadeOut(600, function(){$(this).remove();});
            });
        });

        function fixVideoAd(action, height) {
            //Ad video has been detect
            if($('div[id^="clone_teaser_"]').length) {

                var ad_div = $('div[id^="teaser_"]');

                if(action == "shown") {
                    ad_div.css("top", parseInt(ad_div.css('top').replace(/[^-\d\.]/g, '')) + height);
                }
                else {
                    ad_div.css("top", parseInt(ad_div.css('top').replace(/[^-\d\.]/g, '')) - height);
                }
            }
        }

        function showWebsites() {
            if(!$("#show_websites").is(':empty')) {
                fixVideoAd('hidden', $('#show_websites').outerHeight());

                $("#show_websites").empty();
            }
            else {
                $.get("https://avatars.alphacoders.com/site/show-websites",
                    function(data) {
                        $("#show_websites").html(data);

                        fixVideoAd('shown', $('#show_websites').outerHeight());
                    }
                );
            }
        }

        function handleApiError(error_message) {
            if(!$('.notice-error').length) {
                $('#notice_container').html("<div class='notice notice-error'>" +
                        "<span class='close-notice pull-right' title=\"Close This Notification!\">" +
                            "<span class=\"glyphicon glyphicon-remove\"></span>" +
                        "</span>" +
                        "<b></b>" +
                    "</div>");
            }

            $('.notice-error b').html(error_message);
            $('html,body').animate({scrollTop: $('.notice-error').offset().top - 15});
        }
    </script>

    <script>
        // Add the right icon according to checked or unchecked
        $('.sorting-selector').each(function () {
            var element = $(this);

            if( element.find('input').is(':checked') ) {
                element.append("<i class='el el-check sorting-icon'></i>");
            }
            else {
                element.append("<i class='el el-check-empty sorting-icon'></i>");
            }
        });

        // Another "radio" element is selected
        $('.sorting-selector').click(function () {
            var element = $(this);
            var input = element.find('input');
            if( !input.is(':checked') ) {
                $('input[name='+input.attr('name')+']:checked').next().toggleClass('el-check el-check-empty')
                input.prop("checked", true);
                element.find('.sorting-icon').toggleClass('el-check-empty el-check');
            }
        });
    </script>

            <script type="text/javascript">
            //Manage the quick jump button
            $('.quick-jump-btn').click(function() {
                var page_value = $(this).prev().val();
                if( $.isNumeric( page_value ) && page_value > 0 )
                {
                                            window.location.href = "/by_resolution/128?page=" + page_value;
                                    }
                else
                {
                    $(this).prev().css("border-color", "red");
                }
            });

            var nextPage = $("#next_page");
            var prevPage = $("#prev_page");
            prevUrl = prevPage.attr("href");
            nextUrl = nextPage.attr("href");

            document.onkeydown = function(evt) {
                evt = evt || window.event;
                var j = evt.keyCode;
                var x = evt.target||evt.srcElement;
                if(x != "[object HTMLInputElement]") {
                    if (j == 39 && undefined != nextUrl) {
                        window.location = nextUrl;
                    }
                    else if(j == 37 && undefined != prevUrl) {
                        window.location = prevUrl;
                    }
                }
            }
        </script>
    
    
    
    
